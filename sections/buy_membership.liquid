<style>
  .page__title.rich-text__heading {
    display: none;
  }
  #MainContent .page-width.page-width--fixed.section--padding {
    max-width: 100%;
    padding: 0;
  }
  .mob_view {
    display: none;
  }
  .hide {display: none !important;}
  .section_hero {
    padding-block: 30px;
    background: #e1251c;
    display: grid;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    gap: 20px;
    align-items: center;
    .section-title {
      font-weight: 700;
      margin-bottom: 32px;
      font-size: 32px;
      line-height: 1.3;
      color: #ffffff;
    }
    .section-desc {
      font-weight: 400;
      margin-bottom: 32px;
      font-size: 18px;
      line-height: 1.3;
      color: #ffffff;
    }
    .section_left {
      max-width: 320px;
      margin-inline: auto;
    }
    .section-btn .btn {
      text-decoration: none;
    }
    .open-register-popup span {
      color: #e1251c;
    }
    .open-login-popup {
      span {
        color: #ffffff;
      }
      &:hover span {
        color: #e1251c;
      }
    }
    .section-btn .btn:before {
      z-index: -1;
    }
    img.membership_white {
      margin-bottom: 20px;
    }
  }
  .img_text_row {
    background-color: #f7f7f7;
    .main_section_2 {
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      gap: 20px;
      align-items: center;
      padding-block: 30px;
      max-width: 1000px;
      padding-inline: 20px;
      .sec_img {
        max-width: 450px;
        margin: 0 auto;
      }
      .section_title {
        font-size: 30px;
        font-weight: 700;
        color: #000000;
      }
      .section_left {
        text-align: center;
      }
      .section_desc {
        margin: 10px 0;
      }
      a.btn {
        text-decoration: none;
        span {
          color: #ffffff;
        }
        &:hover span {
          color: #e1251c;
        }
      }
    }
    .price-beat-wave {
      background-color: #f7f7f7;
      display: flex;
      svg {
        margin-bottom: -1px;
      }
    }
  }
  .joning-section {
    .section-title {
      font-size: 29px;
      font-weight: 600;
      color: #000;
      margin-bottom: 20px;
    }
    .section-desc {
      font-size: 18px;
      margin-bottom: 20px;
    }
    a.btn {
      text-decoration: none;
    }
    .open-register-popup {
      span {
        color: #ffffff;
      }
      &:hover span {
        color: #e1251c;
      }
    }
    .open-login-popup {
      &:hover span {
        color: #ffffff;
      }
    }
    .section-btn {
      margin-bottom: 20px;
    }
  }
  .choose-member-section {
    padding-block: 2rem;
    .c_d_gray {
      background: #f8fafb;
      color: #2d2e34;
    }
    .c_l_gray {
      background: #ffffff;
      color: #2d2e34;
    }
    .c_d_blue {
      background: #031546;
      color: #ffffff;
    }
    .c_l_blue {
      background: #0f2153;
      color: #ffffff;
    }
    .section-title {
      font-size: clamp(1.5rem, 4vw, 2rem);
      font-weight: 700;
      color: #2d2e34;
      margin-bottom: 1.5rem;
    }
    .section-tabel {
      position: relative;
      border-radius: 12px;
      overflow: hidden;
      box-shadow: 0 4px 24px rgba(0, 0, 0, 0.08);
      max-width: 930px;
      margin: 0 auto;
    }
    table.w-full th.th-elite {
      position: relative;
    }
    .best_value-text {
      position: absolute;
      top: 0;
      right: 0;
      background: #f5d042;
      color: #1a1a1a;
      font-size: 16px;
      font-weight: 800;
      letter-spacing: 0;
      transform-origin: top right;
      z-index: 2;
      border-radius: 0 0 0 6px;
      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
      padding: 4px 50px;
      transform: translate(15%, 160%) rotate(25deg);
    }
    @media (max-width: 767px) {
      .best_value-text {
        transform: translate(5%, -5%) rotate(8deg);
        font-size: 0.65rem;
        padding: 0.3rem 0.6rem;
      }
    }
    table.w-full {
      width: 100%;
      border-collapse: collapse;
      table-layout: fixed;
    }
    table.w-full th,
    table.w-full td {
      width: 50%;
      padding: 1rem 1.25rem;
      vertical-align: middle;
      text-align: center;
    }
    table.w-full thead th {
      padding-top: 1.5rem;
      padding-bottom: 0.75rem;
    }
    table.w-full thead th.th-elite {
      padding-top: 2rem;
    }
    .button_sec a,
    .button-section a.btn {
      display: inline-block;
      text-decoration: none;
      font-weight: 700;
      text-transform: uppercase;
      border-radius: 9999px;
      padding: 0.5rem 1.5rem;
      transition: opacity 0.2s;
      line-height: 1;
      display: inline-flex;
      letter-spacing: 0.5px;
    }
    .button_sec a.open-register-popup {
      background: #e1251c;
      color: #fff !important;
    }
    .button_sec a.open-register-popup:hover {
      opacity: 0.9;
    }
    .elite-button,
    .button-section a.elite-button {
      background: #d4af37;
      color: #1a1a1a !important;
    }
    .elite-button:hover,
    .button-section a.elite-button:hover {
      opacity: 0.92;
    }
    .price-display {
      font-size: 28px;
      font-weight: 800;
      color: #2d2e34;
    }
    .elite-price--large {
      font-size: 28px;
      font-weight: 800;
    }
    .elite-price-sm {
      font-size: 1rem;
      font-weight: 700;
      opacity: 0.95;
    }
    .elite-price {
      font-weight: 700;
      color: #d4af37;
    }
    table.w-full .c_d_blue .elite-price,
    table.w-full .c_d_blue .elite-price-sm {
      color: #f5d042;
    }
    @media (max-width: 767px) {
      .choose-member-section table.w-full th,
      .choose-member-section table.w-full td {
        padding: 0.75rem 0.5rem;
        font-size: 0.9rem;
      }
      .choose-member-section .price-display,
      .choose-member-section .elite-price--large {
        font-size: 1.25rem;
      }
    }
  }
  @media screen and (max-width: 767px) {
    .desk_view {
      display: none;
    }
    .mob_view {
      display: block;
    }
    .section_hero {
      display: flex;
      flex-direction: column-reverse;
      text-align: center;
      img.membership_white {
        margin-inline: auto;
      }
      .section-title {
        margin-bottom: 20px;
        font-size: 28px;
      }
      .section-desc {
        margin-bottom: 20px;
      }
      .section_left {
        max-width: 100%;
      }
    }
    .img_text_row {
      .main_section_2 {
        display: flex;
        flex-direction: column;
        text-align: center;
        .section_title {
          font-size: 30px;
          font-weight: 700;
          color: #000000;
        }
      }
    }
    .joning-section {
      .section-title {
        font-size: 22px;
      }
    }
    .choose-member-section {
      table.w-full th,
      table.w-full td {
        font-size: 14px;
      }
    }
  }
  @media screen and (max-width: 575px) {
    .choose-member-section {
      table.w-full th,
      table.w-full td {
        font-size: 12px;
      }
    }
  }
