                endif
              endif
            endfor
          -%}
          <div class="cart__footer-wrapper grid gap-6">

            {% comment %} if elite is false and elite customer is false {% endcomment %}
            {% if elite__customer == false and elite == false%}
              <us-cart-membership data-for="club">
                <details class="membership_details new-refresh_price_cart_radio club"  {% if customer == blank %} open {% endif %}>
                  <summary>
                    <label class="new-refresh_price_cart_radio__header" for="cart_club_radio">
                      <input type="radio" name="membership" value="club" id="cart_club_radio" {% if club_price_active %} checked {% endif %}/>
                      <div class="new_custom_radio"></div>
                      <div class="radio__logo_text"> 
                        {% render 'cc-logo-white' %}
                        {% if customer %}
                          <span>Club Member</span>
                        {% else %}
                          <span>Join Member Club</span>
                        {% endif %}
                      </div>
                      <div class="radio__badge">{% render 'radio-bag' %} <span>Free</span></div>
                    </label>
                  </summary>
                  <div class="new-refresh_price_cart_radio_content">
                    <div class="club__elite_saving">  
                      {% assign savings = base_cart_price |  minus: club_cart_price %} 
                      {% comment %} {{ base_cart_price }} {{ club_cart_price }} {% endcomment %}
                      {% if customer %}
                        You are saving <i>{{ savings |  money }}</i> instantly on this order as a Club Member 
                      {% else %} 
                        Save <i>{{ savings |  money }}</i> instantly on this order as a Grasscity Member 
                      {% endif %}
                    </div>
                    <ul>
                      <li>{% render 'check-green' %} Unlock unbeatable prices</li>
                      <li>{% render 'check-green' %} FREE shipping above $50 across top smoking accessory & CBD sites.</li>
                    </ul>
                    {% unless customer %}
                      <div class="flex gap-2">
                        <button class="SSO_popup_login">Already a member? <strong>Log in</strong></button>
                        <p class="orrr">OR</p>
                        <button class="SSO_popup_register"><strong>Join Now</strong></button>
                      </div>
                    {% endunless %}
                  </div>
                </details>
              </us-cart-membership>
            {% endif %}

            {% comment %} only show if customer is not available or if customer is available than not an elite {% endcomment %}
            {% if customer == blank or elite__customer == false %}
              <us-cart-membership data-for="elite" hhh>
                <details class="membership_details new-refresh_price_cart_radio elite" {% if elite_price_active or customer != blank %} open {% endif %}>
                  <summary>
                    <label for="cart_elite_radio" class="new-refresh_price_cart_radio__header">
                      <input type="radio" name="membership" value="elite" id="cart_elite_radio" {% if elite_price_active %} checked {% endif %}/>
                      <div class="new_custom_radio"></div>
                      <div class="radio__logo_text">
                        <span>Save even more with</span>
                        {% render 'elite-txt' %}
                      </div>
                      <div class="radio__badge">{% render 'radio-bag' %}<span>$30</span></div>
                    </label>
                  </summary>
                  <div class="new-refresh_price_cart_radio_content">
                    <div class="elite_unlock_head">  
                        Unlock the ELITE experience 
                        <div class="radio__badge">{% render 'cart-elite-badge' %}<span>Premium</span></div>
                    </div>
                    <p class="single_line_perk">
                      For only <strong><span>$30/year</span></strong>
                      get <strong>all benefits from Grasscity</strong> plus VIP perks like:
                    </p>
                    <ul>
                      <li>{% render 'check-green' %} Save an extra {{ club_cart_price |  minus: elite_cart_price |  money }} instantly at checkout</li>
                      <li>{% render 'check-green' %} Access ELITE exclusive every day low prices</li>
                      <li>{% render 'check-green' %} FREE shipping & returns</li>
                      <li>{% render 'check-green' %} FREE gifts & more</li>
                    </ul>
                    {% unless customer %}
                    <button class="SSO_popup_elite_join"><strong>Join Elite Now</strong></button>
                    {% endunless %}
                  </div>
                </details>
              </us-cart-membership>
            {% endif %}
            
            {% comment %} if customer is an elite {% endcomment %}
            {% if elite__customer %}
              <us-cart-membership data-for="elite">
                <details class="membership_details new-refresh_price_cart_radio elite" open> 
                  <summary>
                    <label for="cart_elite_radio" class="new-refresh_price_cart_radio__header">
                      <input type="radio" name="membership" value="elite" id="cart_elite_radio" checked/>
                      <div class="new_custom_radio"></div>
                      <div class="radio__logo_text">  
                        <span style="font-size: 11px;">You have the ultimate savings with</span>
                        {% render 'elite-txt' %}
                      </div>
                      <div class="radio__badge">
                        {% render 'cart-elite-badge' %}
                        <span style="text-transform: uppercase;">Premium</span>
                      </div>
                    </label> 
                  </summary>
                  <div class="new-refresh_price_cart_radio_content">
                    <div class="club__elite_saving">  
                      {% assign savings = base_cart_price |  minus: elite_cart_price %}  
                      You are saving <i>{{ savings |  money }}</i> instantly on this order as an Elite Member  
                    </div>
                  </div>
                </details>
              </us-cart-membership>
            {% endif %}

            {% comment %} only show if customer is not available or elite is not in cart {% endcomment %}
            {% unless customer or elite == true %}
              <us-cart-membership data-for="base">
                <div class="membership_details new-refresh_price_cart_radio base">
                  <label for="cart_base_radio" class="new-refresh_price_cart_radio__header">
                  <input type="radio" name="membership" value="base" id="cart_base_radio" {% if base_price_active %} checked {% endif %}/>
                    <div class="new_custom_radio"></div>
                    <div class="radio__logo_text"> 
                      <span>Continue as guest at full price</span>
                    </div> 
                  </label>
                </div>
              </us-cart-membership>
            {% endunless %}

            {%- render 'gift-wrapping',
              section_id: section.id,
              gift_wrapping: gift_wrapping,
              gift_wrap_id: gift_wrap_id,
              gift_wraps_in_cart: gift_wraps_in_cart,
              items_in_cart: items_in_cart
            -%}
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when '@app' -%}
                  {%- render block -%}
                {%- when 'subtotal' -%}
                  <div class="cart__footer--subtotal" data-cart-subtotal {{ block.shopify_attributes }}>
                    <div class="grid gap-1">
                      {%- liquid
                        assign discount_codes = cart.cart_level_discount_applications | where: 'type', 'discount_code' | map: 'title'
                        assign cart_items_has_discount = false
                        for item in cart.items
                          for allocation in item.line_level_discount_allocations
                            if allocation.discount_application.type == 'discount_code'
                              assign cart_items_has_discount = true
                            endif
                          endfor
                        endfor
                      -%}
                      {%- if cart.cart_level_discount_applications.size > 0 or cart_items_has_discount -%}
                        <ul
                          class="cart__discounts discounts list-unstyled flex flex-wrap gap-1"
                          role="list"
                          aria-label="{{ 'customer.order.discount' | t }}"
                        >
                          {%- for discount in cart.cart_level_discount_applications -%}
                            <li
                              class="discount text-sm-extra font-body-bold flex items-center"
                              data-discount-code="{{ discount.title }}"
                            >
                              {% render 'icon-discount' %}
                              <span>{{- discount.title -}}</span>
                              <span class="font-body-bold">(-{{ discount.total_allocated_amount | money }})</span>
                              <button
                                is="cart-discount-remove"
                                type="button"
                                aria-label="{{ 'general.cart.coupon.remove' | t: code: discount.title }}"
                                class="btn btn--inherit btn-remove"
                              >
                                {% render 'icon-close', size: 'extra-small' %}
                                {%- render 'loading-spinner' -%}
                              </button>
                            </li>
                          {%- endfor -%}
                          {%- if cart_items_has_discount -%}
                            {%- for item in cart.items -%}
                              {%- for allocation in item.line_level_discount_allocations -%}
                                {%- liquid
                                  unless allocation.discount_application.type == 'discount_code'
                                    continue
                                  endunless

                                  assign discount = allocation.discount_application

                                  assign is_displayed = discount_codes | has: discount.title
                                  if is_displayed
                                    continue
                                  endif

                                  assign discount_codes = item.line_level_discount_allocations | slice: forloop.index0 | map: 'discount_application' | map: 'title' | concat: discount_codes
                                -%}
                                <li
                                  class="discount text-sm-extra font-body-bold flex items-center"
                                  data-discount-code="{{ discount.title }}"
                                >
                                  {% render 'icon-discount' %}
                                  <span>{{- discount.title -}}</span>
                                  <span class="font-body-bold">(-{{ discount.total_allocated_amount | money }})</span>
                                  <button
                                    is="cart-discount-remove"
                                    type="button"
                                    aria-label="{{ 'general.cart.coupon.remove' | t: code: discount.title }}"
                                    class="btn btn--inherit btn-remove"
                                  >
                                    {% render 'icon-close', size: 'extra-small' %}
                                    {%- render 'loading-spinner' -%}
                                  </button>
                                </li>
                              {%- endfor -%}
                            {%- endfor -%}
                          {%- endif -%}
                        </ul>
                      {%- endif -%}
